<div style="padding-top:20px;margin-left:183px;">
  <h2>Listing Chapter</h2><br /><br />
  <% if @chapters.present? %>
  <table>
    <tr>
      <th>Chapter</th>
    </tr>

    <% @chapters.each do |chapter| %>
      <tr>
        <td><%= chapter.name %></td>
      </tr>
    <% end %>
  </table><br />
  <% end %>
  <%= link_to 'Back', courses_path %>
</div>
